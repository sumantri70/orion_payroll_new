<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".form.transaksi.PenggajianInput">

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_tab"
        android:layout_width="0dp"
        android:layout_height="437dp"
        android:layout_below="@+id/stl_tabs"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stl_tabs"
        app:layout_constraintVertical_bias="0.0"></android.support.v4.view.ViewPager>

    <com.example.user.orion_payroll_new.form.transaksi.SlidingTabLayout
        android:id="@+id/stl_tabs"
        android:layout_width="368dp"
        android:layout_height="50dp"
        android:layout_below="@+id/toolbar"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="212dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </com.example.user.orion_payroll_new.form.transaksi.SlidingTabLayout>


</android.support.constraint.ConstraintLayout>